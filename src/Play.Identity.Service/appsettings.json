{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceSettings": {
    "ServiceName": "Identity"
  },
  "MongoDbSettings": {
    "Host": "localhost",
    "Port": 27017,
    "DbName": "Identity"
  },
  "RabbitMqSettings": {
    "Host": "localhost",
    "Prefix": "Identity"
  },
  "IdentitySettings": {
    "AdminUserEmail": "admin@play.com"
  },
  "IdentityServerSettings": {
    "ApiScopes": [
      {
        "Name": "catalog.fullaccess"
      },
      {
        "Name": "catalog.readaccess"
      },
      {
        "Name": "catalog.writeaccess"
      },
      {
        "Name": "inventory.fullaccess"
      },
      {
        "Name": "trading.fullaccess"
      },
      {
        "Name": "IdentityServerApi"
      }
    ],
    "ApiResources": [
      {
        "Name": "catalog",
        "Scopes": [
          "catalog.fullaccess",
          "catalog.readaccess",
          "catalog.writeaccess"
        ],
        "UserClaims": [
          "role"
        ]
      },
      {
        "Name": "inventory",
        "Scopes": [
          "inventory.fullaccess"
        ],
        "UserClaims": [
          "role"
        ]
      },
      {
        "Name": "trading",
        "Scopes": [
          "trading.fullaccess"
        ],
        "UserClaims": [
          "role"
        ]
      },
      {
        "Name": "identity",
        "Scopes": [
          "IdentityServerApi"
        ],
        "UserClaims": [
          "role"
        ]
      }
    ]
  }
}